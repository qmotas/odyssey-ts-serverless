    "graphql": "^16.6.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/typescript": "^3.0.0",
    "@graphql-codegen/typescript-resolvers": "^3.0.0",
    "@serverless/typescript": "^3.27.0",
    "@types/aws-lambda": "^8.10.110",
    "@types/node": "^18.13.0",
    "concurrently": "^7.6.0",
    "esbuild": "^0.16.17",
    "eslint": "^8.34.0",
